
<div class="container">
    <div class="row justify-content-between .no-gutters">
        <header class="tracks-header col-12 container">
            <div class="row justify-content-between align-items-center full-height">
                <h2 class="col-3 text-align-center">Title</h2>
                <h2 class="col-3 text-align-center">Listen</h2>
            </div>
        </header>
        <ul class="col-12 container-fluid">
        {{#each items}}
            <li class="row justify-content-between align-items-center track">
                <h3 class="col">{{this.name}}</h3>
                <audio src={{this.preview_url}} controls="display" class="col"></audio>
            </li>
        {{/each}}
        </ul>

    </div>
</div>